{
  "name": "sharify",
  "version": "2.0.0",
  "description": "A share component to share your website on multiple third-party social networks",
  "main": "src/index.js",
  "scripts": {
    "build": "rm -rf dist && npm run rollup:dev",
    "publish": "rm -rf dist && npm run rollup:dev && npm run rollup:prod",
    "rollup:watch": "rollup -w -c rollup.config.dev.js",
    "rollup:dev": "rollup -c rollup.config.dev.js",
    "rollup:prod": "rollup -c rollup.config.prod.js",
    "lint": "eslint src",
    "flow": "flow status"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JustClear/sharify.git"
  },
  "keywords": [
    "sharify",
    "library"
  ],
  "author": "JustClear",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JustClear/sharify/issues"
  },
  "homepage": "https://github.com/JustClear/sharify",
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^2.13.1",
    "flow-bin": "^0.27.0",
    "rollup": "^0.33.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-json": "^2.0.1",
    "rollup-plugin-sourcemaps": "^0.3.4",
    "rollup-plugin-uglify": "^1.0.1"
  }
}
